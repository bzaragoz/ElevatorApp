        <Button x:Name="btnClose" Content="Close Doors" HorizontalAlignment="Left" Margin="59,294,0,0" VerticalAlignment="Top" Width="75" Click="btnClose_Click"/>
        <Label x:Name="lblKobe" Content="KONE" HorizontalAlignment="Left" Margin="203,247,0,0" VerticalAlignment="Top" FontSize="16" FontFamily="Segoe WP Black"/>
        <Label x:Name="lblOtis" Content="Otis" HorizontalAlignment="Left" Margin="284,243,0,0" VerticalAlignment="Top" FontSize="16" FontFamily="Yu Gothic"/>
        <Label x:Name="lblSchindler" Content="Schindler" HorizontalAlignment="Left" Margin="340,251,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Stencil"/>
        <Label x:Name="lblThyssen" Content="Thyssen" HorizontalAlignment="Left" Margin="442,242,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Script MT Bold"/>
        <Label x:Name="lblKrupp" Content="Krupp" HorizontalAlignment="Left" Margin="450,253,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Script MT Bold"/>
        <Button x:Name="btnAlarm" Content="Alarm" HorizontalAlignment="Left" Margin="59,328,0,0" VerticalAlignment="Top" Width="75" Background="Red" BorderBrush="White" Foreground="White" Click="btnAlarm_Click" FontFamily="Showcard Gothic" FontSize="15"/>
    </Grid>
</Window>
