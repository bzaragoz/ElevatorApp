        <Label x:Name="lblOtisStatus" Content="Ready" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="283,295,0,0" Foreground="#FF2AB606" MouseDoubleClick="lblStop_MouseDoubleClick"/>
        <Label x:Name="lblSchindlerStatus" Content="Ready" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="359,295,0,0" Foreground="#FF2AB606" MouseDoubleClick="lblStop_MouseDoubleClick"/>
        <Label x:Name="lblThyssenKruppStatus" Content="Ready" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="450,295,0,0" Foreground="#FF2AB606" MouseDoubleClick="lblStop_MouseDoubleClick"/>
