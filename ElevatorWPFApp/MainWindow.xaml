        <Label x:Name="lblOtisFloor" Content="Label" HorizontalAlignment="Left" Margin="285,272,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblThyssenKruppFloor" Content="Label" HorizontalAlignment="Left" Margin="451,272,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblSchindlerFloor" Content="Label" HorizontalAlignment="Left" Margin="360,272,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cboxElevator" HorizontalAlignment="Left" Margin="36,221,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem>Kone</ComboBoxItem>
            <ComboBoxItem>Otis</ComboBoxItem>
            <ComboBoxItem>Schindler</ComboBoxItem>
            <ComboBoxItem>Thyssen Krupp</ComboBoxItem>
        </ComboBox>
        <Label x:Name="lblKoneStop" Content="Stop" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="213,318,0,0" Foreground="Gray" MouseDoubleClick="lblStop_MouseDoubleClick"/>
        <Label x:Name="lblOtisStop" Content="Stop" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="287,318,0,0" Foreground="Gray" MouseDoubleClick="lblStop_MouseDoubleClick"/>
        <Label x:Name="lblSchindlerStop" Content="Stop" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="363,318,0,0" Foreground="Gray" MouseDoubleClick="lblStop_MouseDoubleClick"/>
        <Label x:Name="lblThyssenKruppStop" Content="Stop" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="454,318,0,0" Foreground="Gray" MouseDoubleClick="lblStop_MouseDoubleClick"/>
