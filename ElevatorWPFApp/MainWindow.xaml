        <Button x:Name="btnTwo" Content="2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="64,134,0,0" Click="btn_Click" Height="30"/>
        <Button x:Name="btnThree" Content="3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="64,99,0,0" Click="btn_Click" Height="30"/>
        <Button x:Name="btnFour" Content="4" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="64,64,0,0" Click="btn_Click" Height="30"/>
        <Button x:Name="btnFive" Content="5" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="64,29,0,0" Click="btn_Click" Height="30"/>
        <Button x:Name="btnSix" Content="6" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="99,169,0,0" Click="btn_Click" Height="30"/>
        <Button x:Name="btnSeven" Content="7" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="99,134,0,0" Click="btn_Click" Height="30"/>
        <Button x:Name="btnEight" Content="8" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Margin="99,99,0,0" Click="btn_Click" Height="30"/>
